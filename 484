#include<bits/stdc++.h>
using namespace std;

int main()
    {
        map<int,int>mp;
       // map<int,int>::iterator it;
       vector<int>v;
      int n;
        while(cin>>n){
           if(mp[n]==0) v.push_back(n);
           mp[n]++;
        }
       for(int i=0;i<v.size();i++){
        cout<<v[i]<<" "<<mp[v[i]] <<endl;
       }
    }
